{
  "kind": "collectionType",
  "collectionName": "films",
  "info": {
    "singularName": "film",
    "pluralName": "films",
    "displayName": "film"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "author": {
      "type": "string"
    },
    "published": {
      "type": "date"
    },
    "cover": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "plot": {
      "type": "text"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "film"
    }
  }
}
